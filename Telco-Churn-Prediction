    "#df.groupby(\"GENDER\").agg({\"CHURN\":[\"sum\",\"mean\"]})\n",
    "\n",
    "def target_summary_cat(dataframe,target,cat_cols):\n",
    "    for col in cat_cols:\n",
    "        print(dataframe.groupby(col).agg({target:[\"sum\",\"mean\"]}))\n"
   ]
